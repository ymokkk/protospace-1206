<%= form_with model: prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "名称" %><br />
    <%= f.text_field :title, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :detail, "詳細" %><br />
    <%= f.text_area :detail, class: :form__text, id:"prototype_detail" %>
  </div>

  <div class="field">
    <%= f.label :point, "ポイント" %><br />
    <%= f.text_area :point, class: :form__text, id:"prototype_point" %>
  </div>

  <div class="field">
    <%= f.label :image, "画像" %><br />
    <%= f.file_field :image, id:"prototype_image" %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>